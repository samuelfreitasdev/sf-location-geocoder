{
	"name": "web-client",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" --",
		"preview": "vite preview",
		"build-only": "vite build",
		"generate-ts-api": "openapi-typescript http://localhost:8080/v3/api-docs.yaml --output src/api/generated/api.d.ts",
		"type-check": "vue-tsc",
		"type-check-force": "vue-tsc --build --force",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore . ",
		"format": "prettier --ignore-path .gitignore --write src/"
	},
	"dependencies": {
		"@headlessui/vue": "^1.7.20",
		"@vue-leaflet/vue-leaflet": "^0.10.1",
		"@vueuse/core": "^10.9.0",
		"apexcharts": "^3.49.0",
		"autoprefixer": "^10.4.19",
		"core-js": "^3.35.0",
		"leaflet": "^1.9.4",
		"lodash": "^4.17.21",
		"oh-vue-icons": "^1.0.0-rc3",
		"openapi-fetch": "^0.9.3",
		"pinia": "^2.1.7",
		"postcss": "^8.4.38",
		"postcss-cli": "^11.0.0",
		"vue": "^3.4.3",
		"vue-router": "^4.2.5",
		"vue3-apexcharts": "^1.5.2"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.3.3",
		"@tsconfig/node18": "^18.2.2",
		"@types/leaflet": "^1.9.8",
		"@types/lodash": "^4.17.4",
		"@types/node": "^18.19.3",
		"@vitejs/plugin-vue": "^4.5.2",
		"@vitejs/plugin-vue-jsx": "^3.1.0",
		"@vue/eslint-config-prettier": "^8.0.0",
		"@vue/eslint-config-typescript": "^12.0.0",
		"@vue/tsconfig": "^0.5.0",
		"daisyui": "^4.11.1",
		"eslint": "^8.49.0",
		"eslint-plugin-vue": "^9.17.0",
		"npm-run-all2": "^6.1.1",
		"openapi-typescript": "^6.7.5",
		"prettier": "^3.0.3",
		"tailwindcss": "^3.4.4",
		"typescript": "~5.3.0",
		"vite": "^5.0.10",
		"vue-tsc": "^1.8.25"
	}
